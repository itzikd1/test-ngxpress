{
  "version": 2,
  "builds": [
    {
      "src": "dist/ngxpress/server/server.mjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "^/api/(?<path>.*)",
      "dest": "/api/$path"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/ngxpress/server/server.mjs"
    }
  ],
  "buildCommand": "npm run build:with-db",
  "installCommand": "npm install"
}
