{
  "version": 2,
  "builds": [
    {
      "src": "dist/server/server.mjs",
      "use": "@vercel/node"
    },
    {
      "src": "dist/browser/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api"
    },
    {
      "src": "/(.*)",
      "dest": "/server"
    }
  ],
  "buildCommand": "npm run build:with-db",
  "installCommand": "npm install",
  "env": {
    "NODE_ENV": "production"
  },
  "outputDirectory": "dist/browser"
}
